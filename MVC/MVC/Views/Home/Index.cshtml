        <button id="btnAddClient">Add Client [POST]</button>
        <button id="btnDeleteClient">Delete Client [Delete]</button>
        <button id="btnUpdateClient">Update Client [Put]</button>
        <button id="btnGetClient">Get Client [GET]</button>
        <div id="divClients"></div>
        <div id="divClient"></div>
    </section>
    <section>
        <h3>Custom filter Messages</h3>
        <br />
        <span>@ViewBag.CustomActionMessage1</span>
        <br />
        <span>@ViewBag.CustomActionMessage2</span>
        <br />
        <span>@ViewBag.CustomActionMessage3</span>
        <br />
        <span>@ViewBag.CustomActionMessage4</span>
        <br />
    </section>
</div>
